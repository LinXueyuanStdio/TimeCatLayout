    <declare-styleable name="BGANinePhotoLayout">
        <!-- Item 条目圆角尺寸，默认值为 0dp -->
        <attr name="bga_npl_itemCornerRadius" format="dimension|reference"/>
        <!-- 当只有一张图片时，是否显示成大图，默认值为 true -->
        <attr name="bga_npl_showAsLargeWhenOnlyOne" format="boolean"/>
        <!-- Item 间的水平和垂直间距，默认值为 4dp -->
        <attr name="bga_npl_itemWhiteSpacing" format="dimension|reference"/>
        <!-- 出去九宫格部分的空白区域的尺寸，默认值为 100dp -->
        <attr name="bga_npl_otherWhiteSpacing" format="dimension|reference"/>
        <!-- 占位图资源，默认值为 R.mipmap.bga_pp_ic_holder_light -->
        <attr name="bga_npl_placeholderDrawable" format="reference"/>
        <!-- item 的尺寸，优先级高于 bga_npl_otherWhiteSpacing，默认值为 0dp -->
        <attr name="bga_npl_itemWidth" format="dimension|reference"/>
        <!-- 列数,默认值为 3，当该值大于 3 并且数据源里只有四张图片时不会显示成 2 列 -->
        <attr name="bga_npl_itemSpanCount" format="integer"/>
        <!-- 不展开之前最多显示多少图 -->
        <attr name="bga_npl_maxItemDisplayBeforeExpand" format="integer" />
        <!-- 是否展开 -->
        <attr name="bga_npl_isExpand" format="boolean" />
        <!-- 未展开时在最后一张图上显示的蒙层 -->
        <attr name="bga_npl_maskBackground" format="reference" />
        <!-- 蒙层字体颜色 -->
        <attr name="bga_npm_maskTextColor"  format="reference"/>
    </declare-styleable>

    <declare-styleable name="BGAImageView">
        <!-- 默认图片资源，默认值为 null -->
        <attr name="android:src"/>
        <!-- 是否是圆形，默认值为 false -->
        <attr name="bga_iv_circle" format="boolean"/>
        <!-- 圆角矩形的半径，默认值为 0dp -->
        <attr name="bga_iv_cornerRadius" format="reference|dimension"/>
        <!-- 是否是矩形，默认值为 false -->
        <attr name="bga_iv_square" format="boolean"/>
        <!-- 描边的宽度，默认值为 0dp -->
        <attr name="bga_iv_borderWidth" format="reference|dimension"/>
        <!-- 描边的颜色，默认值为 Color.WHITE -->
        <attr name="bga_iv_borderColor" format="reference|color"/>
    </declare-styleable>
    <string name="bga_pp_format_remain_image">+%1$d</string>

    <attr name="BlockMaterialSpinnerStyle" format="reference" />
    <attr name="xui_config_size_spinner_text" format="dimension" />

    <attr name="ms_padding_left_size" format="dimension" />
    <attr name="ms_padding_top_size" format="dimension" />
    <attr name="ms_item_height_size" format="dimension" />

    <attr name="ms_dropdown_offset" format="dimension" />
    <attr name="ms_arrow_size" format="dimension" />
    <declare-styleable name="BlockMaterialSpinner">
        <!--下拉框选择的内容-->
        <attr name="ms_entries" format="reference" />
        <!--下拉框箭头的资源-->
        <attr name="ms_arrow_image" format="reference" />
        <!--下拉框箭头的颜色， 默认和字体的颜色一样-->
        <attr name="ms_arrow_tint" format="color" />
        <!--下拉框是否有箭头， 默认是true-->
        <attr name="ms_hide_arrow" format="boolean" />
        <!--下拉框背景的颜色-->
        <attr name="ms_background_color" format="color" />
        <!--下拉框背景点击的效果-->
        <attr name="ms_background_selector" format="integer" />
        <!--下拉框字体的颜色-->
        <attr name="ms_text_color" format="color" />
        <!--下拉框 下拉条目最大的高度-->
        <attr name="ms_dropdown_max_height" format="dimension" />
        <!--下拉框 下拉条目的样式，默认wrap_content-->
        <attr name="ms_dropdown_height" format="dimension">
            <enum name="fill_parent" value="-1" />
            <enum name="match_parent" value="-1" />
            <enum name="wrap_content" value="-2" />
        </attr>
        <!--下拉框 下拉条目的背景-->
        <attr name="ms_dropdown_bg" format="reference" />
        <!--是否在dialog中, 默认false-->
        <attr name="ms_in_dialog" format="boolean" />
        <!--弹出窗的动画样式-->
        <attr name="ms_pop_anim_style" format="reference" />
    </declare-styleable>
